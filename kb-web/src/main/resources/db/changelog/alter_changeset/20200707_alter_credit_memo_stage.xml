<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
  <changeSet id="3-alter-credit_memo_stage_created_at" author="elvin">
    <preConditions>
      <columnExists tableName="credit_memo_stage" columnName="created_at"/>
    </preConditions>

    <modifyDataType tableName="credit_memo_stage" columnName="created_at" newDataType="DATETIME"/>
  </changeSet>

  <changeSet id="3-alter-credit_memo_stage_last_modified_at" author="elvin">
    <preConditions>
      <columnExists tableName="credit_memo_stage" columnName="last_modified_at"/>
    </preConditions>

    <modifyDataType tableName="credit_memo_stage" columnName="last_modified_at" newDataType="DATETIME"/>
  </changeSet>

</databaseChangeLog>
