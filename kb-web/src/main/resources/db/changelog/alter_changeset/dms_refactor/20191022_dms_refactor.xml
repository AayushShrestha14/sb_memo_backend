<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="alter-memo-bcc-user-constraint" author="nishan">
        <preConditions>
            <foreignKeyConstraintExists foreignKeyTableName="memo_bcc_user" foreignKeyName="fk_memo_bcc_user_memo_id"/>
        </preConditions>
        <dropForeignKeyConstraint baseTableName="memo_bcc_user" constraintName="fk_memo_bcc_user_memo_id"/>
    </changeSet>

    <changeSet id="alter-memo-bcc-user-memo-id" author="nishan">
        <preConditions>
            <columnExists tableName="memo_bcc_user" columnName="memo_id"/>
        </preConditions>
        <dropColumn tableName="memo_bcc_user" columnName="memo_id"></dropColumn>
    </changeSet>

    <changeSet id="alter-memo-cc-user-constraint" author="nishan">
        <preConditions>
            <foreignKeyConstraintExists foreignKeyTableName="memo_cc_user" foreignKeyName="fk_memo_cc_user_memo_id"/>
        </preConditions>
        <dropForeignKeyConstraint baseTableName="memo_cc_user" constraintName="fk_memo_cc_user_memo_id"/>
    </changeSet>

    <changeSet id="alter-memo-cc-user-memo-id" author="nishan">
        <preConditions>
            <columnExists tableName="memo_cc_user" columnName="memo_id"/>
        </preConditions>
        <dropColumn tableName="memo_cc_user" columnName="memo_id"></dropColumn>
    </changeSet>

    <changeSet id="alter-memo-stage-constraint" author="nishan">
        <preConditions>
            <foreignKeyConstraintExists foreignKeyTableName="memo_stage" foreignKeyName="fk_memo_stage_memo_id"/>
        </preConditions>
        <dropForeignKeyConstraint baseTableName="memo_stage" constraintName="fk_memo_stage_memo_id"/>
    </changeSet>

    <changeSet id="alter-memo-stage-memo-id" author="nishan">
        <preConditions>
            <columnExists tableName="memo_stage" columnName="memo_id"/>
        </preConditions>
        <dropColumn tableName="memo_stage" columnName="memo_id"></dropColumn>
    </changeSet>

</databaseChangeLog>


